<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:id="@+id/setLayout"
     >

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10.0dip"
        android:background="@drawable/list_item_f_bg"
        android:paddingLeft="10.0dip"
        android:text="滑动菜单皮肤管理" />

    <GridView
        android:id="@+id/gridview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:horizontalSpacing="10.0dip"
        android:numColumns="4"
        android:stretchMode="columnWidth"
        android:verticalSpacing="10.0dip" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20.0dip"
        android:background="@drawable/list_item_f_bg"
        android:paddingLeft="10.0dip"
        android:text="系统设置" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/downImg_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20.0dip"
            android:layout_marginTop="15.0dip"
            android:text="下载博主头像"
            android:textSize="15.0sp" />

        <CheckBox
            android:id="@+id/downImg_choose"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20.0dip" />

        <TextView
            android:id="@+id/eyes_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/downImg_text"
            android:layout_marginLeft="20.0dip"
            android:layout_marginTop="15.0dip"
            android:text="护眼模式"
            android:textSize="15.0sp" />

        <CheckBox
            android:id="@+id/eyes_choose"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/downImg_text"
            android:layout_marginRight="20.0dip" />
    </RelativeLayout>

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20.0dip"
        android:background="@drawable/list_item_f_bg"
        android:paddingLeft="10.0dip"
        android:text="其他" />

    <Button
        android:id="@+id/feedback_bt"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20.0dip"
        android:layout_marginRight="20.0dip"
        android:layout_marginTop="15.0dip"
        android:background="@drawable/setting_text"
        android:gravity="left"
        android:text="反馈"
        android:textSize="15.0sp" />

    <Button
        android:id="@+id/about_bt"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20.0dip"
        android:layout_marginRight="20.0dip"
        android:layout_marginTop="15.0dip"
        android:background="@drawable/setting_text"
        android:gravity="left"
        android:text="关于"
        android:textSize="15.0sp" />

</LinearLayout>